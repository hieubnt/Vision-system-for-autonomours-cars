        <mxCell id="XJknxzuPZn_kgnMr-SdR-3" value="&lt;h1 style=&quot;margin-top: 0px;&quot;&gt;App API components&lt;/h1&gt;&lt;p&gt;DataCollector&lt;/p&gt;&lt;p&gt;Pipeline&lt;/p&gt;&lt;p&gt;Visualizer&lt;/p&gt;&lt;p&gt;&lt;br&gt;&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" parent="1" vertex="1">
